{
    "name": "rstecinfo/laravel-evolution",
    "description": "Pacote laravel para Evolution API",
    "version": "1.0.1",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RSTecInfo\\LaravelEvolution\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RSTecInfo\\LaravelEvolution\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RSTecInfo\\LaravelEvolution\\Providers\\LaravelEvolutionServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "RSTecInfo",
            "email": "sac@rstecinfo.com.br"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    }
}
